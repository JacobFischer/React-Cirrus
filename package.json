{
  "name": "react-cirrus",
  "version": "0.0.1-pre.1",
  "description": "React/TypeScript components and bindings for Cirrus CSS",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "git@github.com:JacobFischer/React-Cirrus.git",
  "author": "Jacob Fischer <jacob.t.fischer@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/eslint": "7.2.6",
    "@types/html-webpack-plugin": "3.2.4",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.31",
    "@types/react": "17.0.2",
    "@types/react-router-dom": "5.1.7",
    "@types/webpack": "4.41.26",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "cirrus-ui": "0.6.1",
    "classnames": "^2.2.6",
    "eslint": "7.20.0",
    "eslint-config-jacobfischer": "git://github.com/JacobFischer/eslint-config.git#d3ecf89151932989003e837dd11fd8d0715d5a69",
    "eslint-config-jacobfischer-jest": "git://github.com/JacobFischer/eslint-config-jest.git#4cbb83846d4e1a29e95b406ed5d957bdab6b0824",
    "eslint-config-jacobfischer-react": "git://github.com/JacobFischer/eslint-config-react.git#78e497042a4a7e17bec76b475674ef9386ab9cf7",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-import-order-alphabetical": "1.0.1",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-jsdoc": "32.2.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "html-webpack-plugin": "5.2.0",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "npm-check-updates": "11.1.9",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "shx": "0.3.3",
    "ts-loader": "8.0.17",
    "webpack": "5.24.2",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  },
  "peerDependencies": {
    "cirrus-ui": ">=0.6.1",
    "react": ">=16"
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "webpack": "cross-env TS_NODE_PROJECT=\"tsconfig.node.json\" NODE_ENV=\"development\" webpack --progress --mode development --config web/webpack.config.ts",
    "watch": "cross-env TS_NODE_PROJECT=\"tsconfig.node.json\" NODE_ENV=\"development\" webpack serve --progress --mode development --config web/webpack.config.ts",
    "clean": "shx rm -rf .cache/ dist/",
    "purge": "npm run clean && shx rm -rf node_modules/ && rm -rf package-lock.json",
    "lint": "eslint ./ --ext .js,.ts,tsx --ignore-pattern dist/",
    "prettier:base": "prettier **/*.{js,ts,tsx}",
    "prettier": "npm run prettier:base -- --write",
    "prettier:check": "npm run prettier:base -- --check",
    "test": "jest --env=node --config ./test/jest.config.js --colors --coverage test",
    "docs": "typedoc --includeVersion --out docs/ --exclude test/* --entryPoints src/index.ts",
    "docs:predeploy": "shx touch docs/.nojekyll",
    "pack:predeploy": "shx rm -rf ./.*.js && shx cp dist/*.* ./ ",
    "ncu": "ncu -u"
  },
  "keywords": [
    "react",
    "cirrus",
    "cirrus-ui",
    "components",
    "ts",
    "TypeScript"
  ],
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "dependencies": {
    "@types/react-dom": "^17.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.0",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
    "ts-node": "^9.1.1"
  }
}
